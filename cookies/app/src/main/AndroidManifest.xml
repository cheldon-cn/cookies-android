<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chn.cookies">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cookies">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Cookies.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".flexbox.activity.LabelActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Cookies.NoActionBar">
        </activity>
        <activity android:name=".flexbox.activity.CenterGridActivity"
            android:theme="@style/Theme.Cookies.NoActionBar" />
    </application>

</manifest>